{
    "extends": "airbnb/base",
    "env": {        
        "es6": true,
        "node": true       
      },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "class-methods-use-this": 0,
        "comma-dangle": 0
    }    
}