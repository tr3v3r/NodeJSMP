{
    "extends": "airbnb/base",
    "env": {        
        "es6": true,
        "node": true       
      },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "class-methods-use-this": 0,
        "comma-dangle": 0,
        "no-plusplus": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "no-cond-assign": 0,
        "no-use-before-define": 0,
        "object-curly-newline": 0,
        "import/prefer-default-export": 0        
    }    
}